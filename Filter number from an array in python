def filter_list(l):
  'return a new list with the strings filtered out'
  return [x for x in l if type(x) is not str]
  
  
def filter_list(l):
    newl=[]
    for i in l:
      if isinstance(i,int):
         newl.append(i)
         print(newl)
         
     
    return(newl)            
            

l=[1,2,'a','b']
filter_list(l)
